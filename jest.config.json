{
  "verbose": true,
  "moduleNameMapper": {
    "^@app(.*)$": "<rootDir>/src/app$1",
    "^@db(.*)$": "<rootDir>/src/db$1",
    "^@constants(.*)$": "<rootDir>/src/constants$1",
    "^@helpers(.*)$": "<rootDir>/src/helpers$1",
    "^@models(.*)$": "<rootDir>/src/models$1",
    "^@routes(.*)$": "<rootDir>/src/routes$1",
    "^@middlewares(.*)$": "<rootDir>/src/middlewares$1"
  },
  "transform": {
    "^.+\\.js$": "babel-jest"
  }
}
